{
  "version": 2,
  "name": "ai-adhd-web-ui",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@api-url",
    "NEXT_PUBLIC_MCP_CHURNFLOW_URL": "@mcp-churnflow-url",
    "NEXT_PUBLIC_MCP_SHRIMP_URL": "@mcp-shrimp-url"
  }
}